      <!-- build:css styles/main.css -->
      <link rel="stylesheet" href="css/main.css">
      <!-- endbuild -->
  </head>
  <body>
    <div class='main'>
      <div class="ui huge message margin-reset">
          <h1 class="ui huge header">Yorkshire Tech Community</h1>
          <p>There's hot stuff going on in your area!</p>
      </div>
      <div class="ui inverted green menu margin-reset">
        <a class="item" href="calendar">
          <i class="calendar icon"></i> Calendar
        </a>
        <a class="item" href="groups">
          <i class="users icon"></i> Groups
        </a>
        <a class="item" href="conferences">
          <i class="student icon"></i> Conferences
        </a>
        <a class="item" href="elsewhere">
          <i class="marker icon"></i> Elsewhere
        </a>
        <a class="item" href="about">
          <i class="rocket icon"></i> About
        </a>
        <div class="right menu">
          <a class="item" href="http://twitter.com/techyorks">
            <i class="twitter icon"></i> @techyorks
          </a>
        </div>
      </div>
      <div class="ui segment">
        <%=yield%>
      </div>
      <div class="ui divider"></div>  
      <span>&copy; techyorks.uk 2015</span>
    </div>
